{
  "multipart": [
    {
      "apply": {
        "model": "ocm:block/glsgrredpan_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "ocm:block/glsgrredpan_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "ocm:block/glsgrredpan_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "ocm:block/glsgrredpan_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "ocm:block/glsgrredpan_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "ocm:block/glsgrredpan_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "ocm:block/glsgrredpan_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "ocm:block/glsgrredpan_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "ocm:block/glsgrredpan_noside",
        "y": 270
      }
    }
  ]
}